{
  "hosting": {
    "source": ".",
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [{
      "source": "*.@(jpg|jpeg|gif|png|ico)",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=7200"
      } ]
    }, {
      "source": "**/*.@(jpg|jpeg|gif|png|ico)",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=7200"
      } ]
    }, {
      "source": "*.[a-f0-9][a-f0-9][a-f0-9][a-f0-9][a-f0-9][a-f0-9][a-f0-9][a-f0-9].*",
      "headers": [{
        "key": "Cache-Control",
        "value": "public,max-age=31536000,immutable"
      }]
    }, {
      "source": "**/*.[a-f0-9][a-f0-9][a-f0-9][a-f0-9][a-f0-9][a-f0-9][a-f0-9][a-f0-9].*",
      "headers": [{
        "key": "Cache-Control",
        "value": "public,max-age=31536000,immutable"
      }]
    }]
  }
}
